 <MudText Typo="Typo.h6">@L["Open Questions"]</MudText>
  <MudSpacer />
        <MudGrid Spacing="0" Class="d-flex justify-end">
                <MudItem xs="12" md="5">
   <MudTextField  @bind-Value="searchString1" Placeholder=@L["Search"] Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.Search" IconSize="Size.Medium" Class="d-flex justify-end"></MudTextField>
</MudItem>   
</MudGrid>
<MudTable Items="@OpenQuestions" Dense="true" Hover="true" Bordered="false" Striped="false" Filter="new Func<OpenQuestion,bool>(FilterFunc1)" @bind-SelectedItem="selectedItem1">
    <ToolBarContent>
        <MudButton OnClick="(()=>NoteCreate())" Class="pa-2 mx-2" Variant="Variant.Text" Color="Color.Primary">@L["Add New"]</MudButton>
       <MudSpacer/>
    </ToolBarContent>
    <ColGroup>
        <col />
        <col style="width: 60%;" />
        <col />
        <col />
    </ColGroup>
    <HeaderContent>
        <MudTh>@L["Id"]</MudTh>
        <MudTh>@L["Name"]</MudTh>
        <MudTh>@L["Description"]</MudTh>
        <MudTh>@L["Actions"]</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel=@L["Id"]>@context.Id</MudTd>
        <MudTd DataLabel=@L["Name"]>@context.Name</MudTd>
        <MudTd DataLabel=@L["Description"]>@context.Description</MudTd>
        <MudTd DataLabel=@L["Edit"]>
            <MudButton OnClick="((e)=>NoteUpdate(context))" Color="Color.Tertiary">@L["Edit Question"]</MudButton>
        </MudTd>
    </RowTemplate>
    <PagerContent>
        <MudTablePager RowsPerPageString=@L["Rows per page"] />
    </PagerContent>
</MudTable>