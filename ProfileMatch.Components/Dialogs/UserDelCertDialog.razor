@inject IStringLocalizer<LanguageService> L

<MudDialog>

    <DialogActions>
        <MudButton OnClick="Cancel">@L["No"]</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">@L["Yes"]</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}